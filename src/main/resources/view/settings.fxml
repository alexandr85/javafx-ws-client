<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.*?>
<?import javafx.scene.layout.*?>

<TilePane alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
       <GridPane alignment="CENTER" hgap="10.0" vgap="2.0">
           <columnConstraints>
               <ColumnConstraints hgrow="SOMETIMES" />
               <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
           </columnConstraints>
           <rowConstraints>
               <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
            <RowConstraints />
           </rowConstraints>
          <children>
            <HBox style="-fx-border-color: gray; -fx-border-radius: 3px;">
               <children>
                    <GridPane hgap="10.0">
                        <columnConstraints>
                            <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" />
                            <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" />
                        </columnConstraints>
                        <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        </rowConstraints>
                       <children>
                           <TextField maxWidth="1.7976931348623157E308" prefHeight="25.0" promptText="name" GridPane.rowIndex="1" />
                           <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="25.0" text="Save" GridPane.columnIndex="1" GridPane.rowIndex="1">
                               <cursor>
                                   <Cursor fx:constant="HAND" />
                               </cursor>
                           </Button>
                           <ChoiceBox maxWidth="1.7976931348623157E308" prefHeight="25.0" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="2" />
                        <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="75.0" text="Remove" GridPane.rowIndex="3">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Button>
                        <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="25.0" text="Load" GridPane.columnIndex="1" GridPane.rowIndex="3">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Button>
                        <Label text="Profiles" GridPane.columnSpan="2" GridPane.halignment="CENTER" />
                       </children>
                    </GridPane>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-border-color: gray; -fx-border-radius: 3px;" GridPane.columnIndex="1">
               <children>
                    <GridPane>
                        <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        </columnConstraints>
                        <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        </rowConstraints>
                     <children>
                        <Label text="Properties" GridPane.columnSpan="2" GridPane.halignment="CENTER" />
                     </children>
                    </GridPane>
               </children>
            </HBox>
          </children>
       </GridPane>
   </children>
</TilePane>
