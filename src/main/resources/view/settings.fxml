<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.*?>
<?import javafx.scene.layout.*?>

<TilePane alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane alignment="CENTER" hgap="10.0" vgap="2.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES"/>
            <ColumnConstraints hgrow="SOMETIMES"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints/>
            <RowConstraints/>
        </rowConstraints>

        <!-- Profiles block -->
        <HBox maxHeight="200.0" GridPane.valignment="TOP">
            <padding>
                <Insets bottom="20.0" left="25.0" right="25.0" top="20.0"/>
            </padding>
            <GridPane hgap="15.0" vgap="15.0">
                <columnConstraints>
                    <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES"/>
                    <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0"/>
                </rowConstraints>
                <TextField maxWidth="1.7976931348623157E308" prefHeight="25.0" promptText="name"
                           GridPane.rowIndex="1"/>
                <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="25.0"
                        text="Save" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <cursor>
                        <Cursor fx:constant="HAND"/>
                    </cursor>
                </Button>
                <Label alignment="CENTER" maxHeight="1.7976931348623157E308"
                       maxWidth="1.7976931348623157E308" style="-fx-font-size: 16px;" text="Profiles"
                       underline="true" GridPane.columnSpan="2">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </Label>
                <GridPane hgap="15.0" style="-fx-border-color: #e6e6e6; -fx-border-radius: 4px;" vgap="5.0"
                          GridPane.columnSpan="2" GridPane.rowIndex="2">
                    <columnConstraints>
                        <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                    <ChoiceBox maxWidth="1.7976931348623157E308" GridPane.columnSpan="2"/>
                    <Button mnemonicParsing="false" prefWidth="80.0" text="Remove" GridPane.rowIndex="1"/>
                    <Button mnemonicParsing="false" prefWidth="80.0" text="Load" GridPane.columnIndex="1"
                            GridPane.rowIndex="1"/>
                </GridPane>
            </GridPane>
        </HBox>

        <!-- Properties block -->
        <HBox GridPane.columnIndex="1">
            <padding>
                <Insets bottom="20.0" left="25.0" right="25.0" top="20.0"/>
            </padding>
            <GridPane vgap="10.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <Label alignment="CENTER" maxHeight="1.7976931348623157E308"
                       maxWidth="1.7976931348623157E308" style="-fx-font-size: 16px;" text="Properties"
                       underline="true" GridPane.columnSpan="2" GridPane.halignment="CENTER">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </Label>
                <GridPane hgap="5.0" vgap="5.0" GridPane.columnSpan="2" GridPane.rowIndex="1">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="87.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <CheckBox mnemonicParsing="false" text="Check new version" GridPane.columnSpan="2"
                              GridPane.rowIndex="0"/>
                    <CheckBox mnemonicParsing="false" text="Default json pretty " GridPane.columnSpan="2"
                              GridPane.rowIndex="1"/>
                    <Button maxHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                            prefWidth="70.0" text="Save" GridPane.columnSpan="2"
                            GridPane.halignment="RIGHT" GridPane.rowIndex="4"/>
                    <Slider blockIncrement="1.0" max="18.0" min="10.0" value="13.0" GridPane.rowIndex="3"/>
                    <Label text="Font size:" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="3"/>
                    <Label alignment="CENTER" text="13" GridPane.columnIndex="1" GridPane.halignment="RIGHT"
                           GridPane.rowIndex="3"/>
                    <TextField promptText="regex for json pretty message" GridPane.columnSpan="2"
                               GridPane.rowIndex="2"/>
                </GridPane>
            </GridPane>
        </HBox>
    </GridPane>
</TilePane>
